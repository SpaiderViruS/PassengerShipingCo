<UserControl x:Class="PassengerShipingCo.UserControls.PassengerCruiseControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PassengerShipingCo.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="800" Loaded="UserControl_Loaded">
    <Border BorderBrush="Black" BorderThickness="3">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <StackPanel Margin="5" Orientation="Vertical" HorizontalAlignment="Left">
                <Label x:Name="SNPLabel"/>
                <Label x:Name="TourLabel"/>
            </StackPanel>
        </Grid>
    </Border>
</UserControl>
